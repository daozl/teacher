#include <stdio.h>
#include <stdlib.h>

int main()
{
	int ch;

	ch = getchar();

	switch(ch)
	{
		case 'a':
		case 'A':
			printf("Ant");
			break;
		case 'b':
		case 'B':
			printf("Butterfly");
			break;
		case 'c':
        case 'C':
            printf("Cobra");
            break;
		case 'd':
        case 'D':
            printf("Donkey");
            break;
		default:
			printf("Error");
			exit(1);
	}

	exit(0);
}


